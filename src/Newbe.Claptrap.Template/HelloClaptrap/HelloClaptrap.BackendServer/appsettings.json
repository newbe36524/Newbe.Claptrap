{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Claptrap": {
    "DefaultConnectionString": "see in tye.yml",
    "EventStore": {
      "Strategy": "SharedTable",
      "DatabaseType": "Sqlite",
      "InsertManyWindowCount": 1500
    },
    "StateStore": {
      "Strategy": "SharedTable",
      "DatabaseType": "Sqlite",
      "InsertManyWindowCount": 2000
    }
  }
}
